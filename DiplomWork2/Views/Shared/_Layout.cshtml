<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - EasyCod</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/DiplomWork2.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"><img href="/" class="logo" src="~/css/logo.svg" alt="Learning Web" /></a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                
                  <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1" id="pills-tab" role="tablist">
                       
                        <li class="nav-item" role="presentation">
                                <a class="nav-link text-dark" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                      
                        <li class="nav-item" role="presentation">
                            <a class="nav-link text-dark" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" asp-area="" asp-controller="Assembler" asp-action="Index">Assembler</a>
                        </li>
                        <li class="nav-item" role="presentation">
                            <a class="nav-link text-dark" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" asp-area="" asp-controller="C_" asp-action="Index">C#</a>
                      
                        </li class="nav-item">
                        <li class="nav-item" role="presentation">
                            <a class="nav-link text-dark" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" asp-area="" asp-controller="CSS" asp-action="Index">CSS</a>
                        
                        </li>
                        <li class="nav-item" role="presentation">
                            <a class="nav-link text-dark" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" asp-area="" asp-controller="HTML" asp-action="Index">HTML</a>
                       
                        </li>
                        <li class="nav-item" role="presentation">
                            <a class="nav-link text-dark" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" asp-area="" asp-controller="PYTHON" asp-action="Index">Python</a>
                     
                        </li>
                        <li class="nav-item" role="presentation">
                            <a class="nav-link text-dark" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" asp-area="" asp-controller="SQL" asp-action="Index">SQL</a>
                       
                        </li>
                      </ul>
                    </div>
                   </div>
                   <partial name="_LoginPartial" />
                
            
        </nav>
    </header>
    
    <div class="container">
        <div>
           
        </div>
             <main role="main" class="pb-3">
            @RenderBody()
             </main>
    </div>
   
    <div>
    
    </div>
    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - DiplomWork2 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
    
</body>
</html>
